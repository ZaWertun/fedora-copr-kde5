msgstr "Erro de conexión"

#: configwidget.cpp:74
#, kde-format
msgid "Tested connection successfully."
msgstr "A conexión probouse satisfactoriamente."

#: configwidget.cpp:74
#, kde-format
msgid "Connection success"
msgstr "Conexión satisfactoria"

