#: configwidget.cpp:25
#, kde-format
msgid "Enter the http or https URL to your Open-Xchange installation here."
msgstr "Insira aquí o URL http ou https da instalación de Open-Xchange."

#: configwidget.cpp:26
#, kde-format
msgid "Enter the username of your Open-Xchange account here."
msgstr "Insira aquí o nome de usuario da conta de Open-Xchange."

#: configwidget.cpp:27
#, kde-format
msgid "Enter the password of your Open-Xchange account here."
msgstr "Insira aquí o contrasinal da conta de Open-Xchange."

#: configwidget.cpp:72
#, kde-format
msgid "Unable to connect: %1"
msgstr "Non se pode conectar: %1"

#: configwidget.cpp:72
