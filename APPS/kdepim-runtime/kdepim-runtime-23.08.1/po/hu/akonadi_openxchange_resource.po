#: configwidget.cpp:25
#, kde-format
msgid "Enter the http or https URL to your Open-Xchange installation here."
msgstr "Adja meg az Open-Xchange telepítésének http vagy https URL-címét."

#: configwidget.cpp:26
#, kde-format
msgid "Enter the username of your Open-Xchange account here."
msgstr "Adja meg az Open-Xchange azonosítójának felhasználónevét."

#: configwidget.cpp:27
#, kde-format
msgid "Enter the password of your Open-Xchange account here."
msgstr "Adja meg az Open-Xchange azonosítójának jelszavát."

#: configwidget.cpp:72
#, kde-format
msgid "Unable to connect: %1"
msgstr "Nem sikerült csatlakozni: %1"

#: configwidget.cpp:72
#, kde-format
msgid "Connection error"
msgstr "Csatlakozási hiba"

#: configwidget.cpp:74
#, kde-format
msgid "Tested connection successfully."
msgstr "A kapcsolat sikeresen tesztelve."

#: configwidget.cpp:74
#, kde-format
msgid "Connection success"
msgstr "A kapcsolódás sikeres"

