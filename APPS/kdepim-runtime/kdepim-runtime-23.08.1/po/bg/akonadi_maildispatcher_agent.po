#: maildispatcheragent.cpp:84 maildispatcheragent.cpp:233
#, kde-format
msgid "Sending messages (1 item in queue)..."
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "Изпращане на съобщения (1 елемент в опашката)..."
msgstr[1] "Изпращане на съобщения (%1 елемента в опашката)..."

#: maildispatcheragent.cpp:94
#, kde-format
msgid "Sending canceled."
msgstr "Изпращането е отменено."

#: maildispatcheragent.cpp:101
#, kde-format
msgid "Finished sending messages."
msgstr "Изпращането на съобщения приключи."

#: maildispatcheragent.cpp:107
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "Електронната поща е изпратена успешно"

#: maildispatcheragent.cpp:108
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "Вашата електронна поща е изпратена успешно."

#: maildispatcheragent.cpp:114
#, kde-format
msgid "No items in queue."
msgstr "Няма елементи в опашката."

#: maildispatcheragent.cpp:164
#, kde-format
msgid "Online, sending messages in queue."
msgstr "Онлайн, изпращане на съобщения на опашката."

#: maildispatcheragent.cpp:168
#, kde-format
msgid "Offline, message sending suspended."
msgstr "Офлайн, изпращането на съобщения е спряно."

#: maildispatcheragent.cpp:193
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "Изпращане: %1"

#: maildispatcheragent.cpp:258
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "Изпращането на електронна поща е неуспешно"

#: maildispatcheragent.cpp:291
#, kde-format
msgid "Ready to dispatch messages."
msgstr "Готовност за изпращане на съобщения."

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "Id на изходяща кутия"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "Идентификатор на колекцията за изпратена поща"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "Не може да се осъществи достъп до папка на изходяща кутия (%1) ."

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "Изпращането на съобщения е прекъснато."

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr ""
"Не можа да се инициира транспортиране на съобщението. Вероятно невалиден "
"транспорт."

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "Не можа да изпрати съобщение. Невалиден транспорт."

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "Не успя да се получи D-Bus интерфейс на ресурс %1."

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "Невалиден D-Bus отговор от ресурс %1."

#: sendjob.cpp:228
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr ""
"Папката по подразбиране за изпратени писма не е налична. Запазване на "
"съобщението в изходящата поща."

#: sendjob.cpp:240
#, kde-format
msgid "Message transport aborted."
msgstr "Транспортът на съобщението е прекъснат."

#: sendjob.cpp:240
#, kde-format
msgid "Failed to transport message."
msgstr "Неуспешно транспортиране на съобщението."

#: sendjob.cpp:282
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "Не успя да получи D-Bus интерфейса на mailfilteragent."

#: sendjob.cpp:289
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "Невалиден D-Bus отговор от mailfilteragent"

#: sendjob.cpp:331
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr ""
"Изпращането е успешно, но не е успяло да премахне съобщението от изходящата "
"кутия."

#: sendjob.cpp:334
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr ""
"Изпращането е успешно, но не успя да премести съобщението в папката за "
"изпратена поща."

#: sendjob.cpp:370
#, kde-format
msgid "Failed to store result in item."
msgstr "Не успя да съхрани резултата в елемента."

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "Неуспешно извличане на резултата от елемента."
