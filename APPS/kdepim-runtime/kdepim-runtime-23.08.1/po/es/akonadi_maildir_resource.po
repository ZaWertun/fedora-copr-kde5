#, kde-format
msgid "The selected path does not exist."
msgstr "La ruta seleccionada no existe."

#: libmaildir/maildir.cpp:214 libmaildir/maildir.cpp:219
#, kde-format
msgid "Error opening %1; this folder is missing."
msgstr "Error al abrir %1; está carpeta se ha perdido."

#: libmaildir/maildir.cpp:225
#, kde-format
msgid ""
"Error opening %1; either this is not a valid maildir folder, or you do not "
"have sufficient access permissions."
msgstr ""
"Error al abrir %1; bien no es una carpeta válida maildir, o usted no dispone "
"de suficientes permisos de acceso."

#: libmaildir/maildir.cpp:452 libmaildir/maildir.cpp:474
#: libmaildir/maildir.cpp:492 libmaildir/maildir.cpp:512
#: libmaildir/maildir.cpp:531 libmaildir/maildir.cpp:555
#: libmaildir/maildir.cpp:630 libmaildir/maildir.cpp:780
#, kde-format
msgid "Cannot locate mail file %1."
msgstr "No se puede localizar el archivo de correo %1."

#: libmaildir/maildir.cpp:458 libmaildir/maildir.cpp:480
#, kde-format
msgid "Cannot open mail file %1."
msgstr "No se puede abrir el archivo de correo %1."

#: libmaildir/maildir.cpp:563 libmaildir/maildir.cpp:590
#, kde-format
msgid "Cannot write to mail file %1."
msgstr "No se puede escribir el archivo de correo %1."

#: libmaildir/maildir.cpp:604
#, kde-format
msgid "Failed to create mail file %1. The error was: %2"
msgstr "Ha fallado al crear el archivo de correo %1. El error fue: %2"

#: libmaildir/maildir.cpp:698
#, kde-format
msgid "Failed to update the file name %1 to %2 on the disk. The error was: %3."
msgstr ""
"Ha fallado al actualizar el archivo denominado %1 a %2 en el disco. El error "
"fue: %3."

#: maildirresource.cpp:122 maildirresource.cpp:254 maildirresource.cpp:293
#: maildirresource.cpp:387 maildirresource.cpp:428 maildirresource.cpp:548
#: maildirresource.cpp:578 maildirresource.cpp:621 maildirresource.cpp:654
#, kde-format
msgid "Unusable configuration."
msgstr "Configuración no utilizable."

#: maildirresource.cpp:187
#, kde-format
msgid "Unable to fetch item: The maildir folder \"%1\" is not valid."
msgstr ""
"No es posible obtener el elemento: La carpeta maildir «%1» no es válida."

#: maildirresource.cpp:265
#, kde-format
msgid "Error: Unsupported type."
msgstr "Error: Tipo no admitido."

#: maildirresource.cpp:331
#, kde-format
msgid "Failed to change the flags for the mail. %1"
msgstr "Ha fallado al cambiar los indicadores para el correo. %1"

#: maildirresource.cpp:366
#, kde-format
msgid "Maildir resource got a non-mail content."
msgstr "Un recurso de Maildir tomó un contenido que no es correo."

#: maildirresource.cpp:393
#, kde-format
msgid "Source folder is invalid: '%1'."
msgstr "La carpeta de origen no es válida: «%1»."

#: maildirresource.cpp:399
#, kde-format
msgid "Destination folder is invalid: '%1'."
msgstr "La carpeta de destino no es válida: «%1»."

#: maildirresource.cpp:416
#, kde-format
msgid "Could not move message '%1' from '%2' to '%3'. The error was %4."
msgstr ""
"No se ha podido mover el mensaje «%1» desde «%2» a «%3». El error fue %4."

#: maildirresource.cpp:438
#, kde-format
msgid "Failed to delete message: %1"
msgstr "Ha fallado al borrar el mensaje: %1"

#: maildirresource.cpp:527
#, kde-format
msgid "Maildir '%1' for collection '%2' is invalid."
msgstr "El Maildir «%1» de la colección «%2» no es válido."

#: maildirresource.cpp:606
#, kde-format
msgid "Unable to rename maildir folder '%1'."
msgstr "No es posible cambiar de nombre la carpeta maildir «%1»."

#: maildirresource.cpp:627
#, kde-format
msgid "Cannot move root maildir folder '%1'."
msgstr "No se puede mover la carpeta maildir raíz «%1»."

#: maildirresource.cpp:642
#, kde-format
msgid "Unable to move maildir folder '%1' from '%2' to '%3'."
msgstr "No es posible mover la carpeta maildir «%1» de «%2» a «%3»."

#: maildirresource.cpp:660
#, kde-format
msgid "Cannot delete top-level maildir folder '%1'."
msgstr "No se puede borrar la carpeta maildir raíz «%1»."

#: maildirresource.cpp:669
#, kde-format
msgid "Failed to delete sub-folder '%1'."
msgstr "Ha fallado borrar la subcarpeta «%1»."

#: maildirresource.cpp:683
#, kde-format
msgid "Unable to create maildir '%1'."
msgstr "No se puede crear el maildir «%1»."

#: maildirresource.cpp:693
#, kde-format
msgid "No usable storage location configured."
msgstr "No se ha configurado una ubicación de almacenamiento utilizable."

