#: maildispatcheragent.cpp:84 maildispatcheragent.cpp:233
#, kde-format
msgid "Sending messages (1 item in queue)..."
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "កំពុង​ផ្ញើសារ (ធាតុ %1 នៅ​ក្នុងជួរ)...​"

#: maildispatcheragent.cpp:94
#, kde-format
msgid "Sending canceled."
msgstr "បានបោះបង់​ការ​ផ្ញើ ។"

#: maildispatcheragent.cpp:101
#, kde-format
msgid "Finished sending messages."
msgstr "បានបញ្ចប់​កា​រផ្ញើសារ ។"

#: maildispatcheragent.cpp:107
#, fuzzy, kde-format
#| msgctxt "Notification when the email was sent"
#| msgid "E-mail successfully sent"
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "បាន​ផ្ញើ​អ៊ីមែល​ដោយ​ជោគជ័យ"

#: maildispatcheragent.cpp:108
#, fuzzy, kde-format
#| msgctxt "Notification when the email was sent"
#| msgid "E-mail successfully sent"
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "បាន​ផ្ញើ​អ៊ីមែល​ដោយ​ជោគជ័យ"

#: maildispatcheragent.cpp:114
#, kde-format
msgid "No items in queue."
msgstr "គ្មាន​ធាតុ​នៅ​ក្នុង​ជួរ​ទេ ។"

#: maildispatcheragent.cpp:164
#, kde-format
msgid "Online, sending messages in queue."
msgstr "លើ​បណ្ដាញ ផ្ញើ​សារ​នៅ​ក្នុង​ជួរ ។"

#: maildispatcheragent.cpp:168
#, kde-format
msgid "Offline, message sending suspended."
msgstr "ក្រៅ​បណ្ដាញ ការ​ផ្ញើសារ​បាន​ផ្អាក ។"

#: maildispatcheragent.cpp:193
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "ផ្ញើ ៖ %1"

#: maildispatcheragent.cpp:258
#, fuzzy, kde-format
#| msgctxt "Notification when email sending failed"
#| msgid "E-mail sending failed: %1"
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "បានបរាជ័​ក្នុងការ​ផ្ញើ​អ៊ីមែល ៖ %1"

#: maildispatcheragent.cpp:291
#, kde-format
msgid "Ready to dispatch messages."
msgstr "ត្រៀម​រួច​ហើយ​ដើម្បី​​បញ្ជូន​​សារ ។"

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "លេខ​សម្គាល់​សម្រាំង​របស់​ប្រអប់​ចេញ"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "លេខ​សម្រាំង​សំបុត្រ​ដែលបានផ្ញើ​ចេញ"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "មិន​អាច​ចូលដំណើរការ​ថត​ប្រអប់​ចេញ​ (%1) បាន​ទេ ។"

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "បាន​បោះបង់​កា​រផ្ញើសារ ។"

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr "មិនអាច​ចាប់ផ្ដើម​ការ​បញ្ជូន​សារ​បាន​ទេ ។ ប្រហែល​ជា​ការ​បញ្ជូន​មិន​ត្រឹមត្រូវ ។"

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "មិន​អាច​ផ្ញើសារ​បាន​ទេ ។ ការ​បញ្ជូន​មិន​ត្រឹមត្រូវ ។"

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "បានបរាជ័យ​ក្នុងការ​ទៅ​យក​ចំណុច​ប្រទាក់ D-Bus របស់​​ធនធាន %1 ។"

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "ការ​ឆ្លើយតប D-Bus មិន​ត្រឹមត្រូវ​ពី​ធនធាន %1 ។"

#: sendjob.cpp:228
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr "ថត​សំបុត្រ​បាន​ផ្ញើ​មិន​ត្រឹមត្រូវ ។ ទុក​សារ​នៅ​ក្នុង​ប្រអប់​ចេញ ។"

#: sendjob.cpp:240
#, kde-format
msgid "Message transport aborted."
msgstr "បានបោះបង់​ការ​បញ្ជូន​សារ ។"

#: sendjob.cpp:240
#, kde-format
msgid "Failed to transport message."
msgstr "បាន​​បរាជ័យ​ក្នុង​ការ​បញ្ជូនសារ ។"

#: sendjob.cpp:282
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "បាន​បរាជ័យ​ក្នុង​ការ​ទៅ​យក​ចំណុច​ប្រទាក់ D-Bus របស់ mailfilteragent ។"

#: sendjob.cpp:289
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "ការ​ឆ្លើយតប D-Bus មិន​ត្រឹមត្រូវ​​ពី mailfilteragent"

#: sendjob.cpp:331
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr "ផ្ញើ​សារ​បាន​ជោគជ័យ ប៉ុន្តែ​បរាជ័យ​ក្នុង​ការ​យក​សារ​ចេញ​ពី​ប្រអប់​ចេញ ។"

#: sendjob.cpp:334
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr "ផ្ញើ​សារ​បាន​​ជោគជ័យ ប៉ុន្តែ​បាន​បរាជ័យ​ក្នុង​ការ​ផ្លាស់ទី​សារ​ទៅកាន់​ថត​សំបុត្រ​ផ្ញើ ។"

#: sendjob.cpp:370
#, kde-format
msgid "Failed to store result in item."
msgstr "បាន​បរាជ័យ​ក្នុង​ការ​ផ្ទុក​លទ្ធផល​នៅ​ក្នុង​ធាតុ ។"

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "បាន​បរាជ័យ​ក្នុងការ​ទៅ​យក​​ធាតុ ។"

