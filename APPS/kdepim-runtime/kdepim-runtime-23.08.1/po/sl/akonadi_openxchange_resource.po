#: configwidget.cpp:25
#, kde-format
msgid "Enter the http or https URL to your Open-Xchange installation here."
msgstr "Sem vnesite naslov URL http ali https vaše namestitve OpenXchange."

#: configwidget.cpp:26
#, kde-format
msgid "Enter the username of your Open-Xchange account here."
msgstr "Sem vnesite uporabniško ime računa Open-Xchange."

#: configwidget.cpp:27
#, kde-format
msgid "Enter the password of your Open-Xchange account here."
msgstr "Sem vnesite geslo računa Open-Xchange."

#: configwidget.cpp:72
#, kde-format
msgid "Unable to connect: %1"
msgstr "Ni mogoče vzpostaviti povezave: %1"

#: configwidget.cpp:72
#, kde-format
