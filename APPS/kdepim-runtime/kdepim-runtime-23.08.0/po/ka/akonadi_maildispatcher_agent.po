#: maildispatcheragent.cpp:84 maildispatcheragent.cpp:233
#, kde-format
msgid "Sending messages (1 item in queue)..."
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "შეტყობინებების გაგზავნა (რიგში 1 შეტყობინებაა)..."
msgstr[1] "შეტყობინებების გაგზავნა (რიგში %1 შეტყობინებაა)..."

#: maildispatcheragent.cpp:94
#, kde-format
msgid "Sending canceled."
msgstr "გაგზავნა გაუქმებულია."

#: maildispatcheragent.cpp:101
#, kde-format
msgid "Finished sending messages."
msgstr "გაგზავნა დასრულებულია."

#: maildispatcheragent.cpp:107
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "ელფოსტა წარმატებით გაიგზავნა"

#: maildispatcheragent.cpp:108
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "თქვენი ელფოსტა წარმატებით გაიგზავნა."

#: maildispatcheragent.cpp:114
#, kde-format
msgid "No items in queue."
msgstr "რიგში ელფოსტა არ არსებობს."

#: maildispatcheragent.cpp:164
#, kde-format
msgid "Online, sending messages in queue."
msgstr "ონლაინ, რიგში მყოფი შეტყობინებების გაგზავნა."

#: maildispatcheragent.cpp:168
#, kde-format
msgid "Offline, message sending suspended."
msgstr "ოფლაინ, შეტყობინებების გაგზავნა შეჩერებულია."

#: maildispatcheragent.cpp:193
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "გაგზავნა: %1"

#: maildispatcheragent.cpp:258
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "ელფოსტის გაგზავნუის შეცდომა"

#: maildispatcheragent.cpp:291
#, kde-format
msgid "Ready to dispatch messages."
msgstr "შეტყობინებების გაშვებისთვის მზადაა."

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "გამავალი კოლექციის ID"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "გაგზავნილი ელფოსტის კოლექციის ID"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "გამავალი ელფოსტების საქაღალდესთან (%1) წვდომის სეცდომა."

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "შეტყოვინების გაგზავნა გაუქმდა."

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr ""
"შეტყობინებების ტრანსპორტის ინიციალიზაციის შეცდომა. შეიძლება ტრანსპორტი "
"არასწორია."

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "შეტყობინების გაგზავნა შეუძლებელია. ტრანსპორტი არასწორია."

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "D-Bus ინტერფეისის მიღების შეცდომა რესურსისთვის: %1."

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "D-Bus-ის არასწორი პასუხი რესურსისთვის: %1."

#: sendjob.cpp:228
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr ""
"გაგზავნილი ელფოსტების საქაღალდე მიუწვდომელია. შეტყობინებები შემოსულებში "
"იქნება შენახული."

#: sendjob.cpp:240
#, kde-format
msgid "Message transport aborted."
msgstr "შეტყობინების ტრანსპორტის შეცდომა."

#: sendjob.cpp:240
#, kde-format
msgid "Failed to transport message."
msgstr "შეტყობინების გადაცემის შეცდომა."

#: sendjob.cpp:282
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "Mailfilteragent-ის D-Bus ინტერფეისის მიღების შეცდომა."

#: sendjob.cpp:289
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "არასწორი D-Bus პასუხი mailfilteragent-დან"

#: sendjob.cpp:331
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr ""
"გაგზავნა წარმატებულია, მაგრამ გასაგზავნი საქაღალდიდან შეტყობინება ვერ "
"წავშალე."

#: sendjob.cpp:334
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr ""
"გაგზავნა წარმატებულია, მაგრამ შეტყობინება გაგზავნილების საქაღალდეში ვერ "
"გადავიტანე."

#: sendjob.cpp:370
#, kde-format
msgid "Failed to store result in item."
msgstr "შედეგის ელემენტში შენახვის შეცდომა."

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "ელემენტის გამოთხოვის სეცდომა."
