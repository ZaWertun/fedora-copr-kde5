#: configwidget.cpp:25
#, kde-format
msgid "Enter the http or https URL to your Open-Xchange installation here."
msgstr "Zadajte http alebo https URL na vašu Open-Xchange inštaláciu."

#: configwidget.cpp:26
#, kde-format
msgid "Enter the username of your Open-Xchange account here."
msgstr "Zadajte používateľa pre váš účet Open-Xchange."

#: configwidget.cpp:27
#, kde-format
msgid "Enter the password of your Open-Xchange account here."
msgstr "Zadajte heslo pre váš účet Open-Xchange."

#: configwidget.cpp:72
#, kde-format
msgid "Unable to connect: %1"
msgstr "Nemôžem pripojiť: %1"

#: configwidget.cpp:72
#, kde-format
