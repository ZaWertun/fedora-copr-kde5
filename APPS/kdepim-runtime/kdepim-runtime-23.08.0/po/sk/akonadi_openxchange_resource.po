msgstr "Chyba pripojenia"

#: configwidget.cpp:74
#, kde-format
msgid "Tested connection successfully."
msgstr "Pripojenie úspešne otestované."

#: configwidget.cpp:74
#, kde-format
msgid "Connection success"
msgstr "Pripojenie úspešné"

