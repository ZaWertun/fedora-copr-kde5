#, kde-format
msgid "Cannot move root maildir folder '%1'."
msgstr "'%1' MailDir түбір қапшығы жылжытылмады."

#: maildirresource.cpp:642
#, kde-format
msgid "Unable to move maildir folder '%1' from '%2' to '%3'."
msgstr "'%1' MailDir қапшығы '%2' дегенен '%3' дегенге жылжытылмады."

#: maildirresource.cpp:660
#, kde-format
msgid "Cannot delete top-level maildir folder '%1'."
msgstr "Жоғары деңгейлі '%1' MailDir қапшығы өшірілмеді."

#: maildirresource.cpp:669
#, kde-format
msgid "Failed to delete sub-folder '%1'."
msgstr "'%1' ішкі қапшықты өшіру жаңылысы."

#: maildirresource.cpp:683
#, kde-format
msgid "Unable to create maildir '%1'."
msgstr "'%1' MailDir қапшығы құрылмады."

#: maildirresource.cpp:693
#, kde-format
msgid "No usable storage location configured."
msgstr "Баптауында жарамсыз сақтағышы көрсетілген."

