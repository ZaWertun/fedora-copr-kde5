msgstr "Ne eblas movi maildir-dosierujon '%1' de '%2' al '%3'."

#: maildirresource.cpp:660
#, kde-format
msgid "Cannot delete top-level maildir folder '%1'."
msgstr "Ne povas forigi la plej altan maildir-dosierujon '%1'."

#: maildirresource.cpp:669
#, kde-format
msgid "Failed to delete sub-folder '%1'."
msgstr "Malsukcesis forigi subdosierujon '%1'."

#: maildirresource.cpp:683
#, kde-format
msgid "Unable to create maildir '%1'."
msgstr "Ne eblas krei maildir '%1'."

#: maildirresource.cpp:693
#, kde-format
msgid "No usable storage location configured."
msgstr "Neniu uzebla konserva loko agordiĝis."

