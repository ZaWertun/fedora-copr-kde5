#, kde-format
msgid "Unable to fetch item: The maildir folder \"%1\" is not valid."
msgstr "Ne eblas preni objekton: La dosierujo de maildir \"%1\" ne validas."

#: maildirresource.cpp:265
#, kde-format
msgid "Error: Unsupported type."
msgstr "Eraro: Nesubtenata tipo."

