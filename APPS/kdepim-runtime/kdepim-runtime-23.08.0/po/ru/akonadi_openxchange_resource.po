msgstr "Ошибка соединения"

#: configwidget.cpp:74
#, kde-format
msgid "Tested connection successfully."
msgstr "Тестирование соединения прошло успешно."

#: configwidget.cpp:74
#, kde-format
msgid "Connection success"
msgstr "Успешно соединено"

