+bool MousePosEffect::init()
+{
+    effects->makeOpenGLContextCurrent();
+    m_lastRect[0].moveCenter(cursorPos());
+    m_lastRect[1].moveCenter(cursorPos());
+    m_active = true;
+    printf("init...\n");
+    return true;
+}
